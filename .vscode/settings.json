{
  "files.associations": {
    "wrangler.json": "jsonc"
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.oxc": "explicit"
  },
  "editor.formatOnSave": true,
  "files.eol": "\n",
  "editor.defaultFormatter": "oxc.oxc-vscode",
  "oxc.fmt.experimental": true,
  "[typescript]": {
    "editor.defaultFormatter": "oxc.oxc-vscode"
  }
}